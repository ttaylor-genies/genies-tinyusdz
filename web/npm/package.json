{
  "name": "tinyusdz",
  "version": "0.9.0",
  "description": "Tinyusdz wasm",
  "main": "tinyusdz.js",
  "files": [
    "./dist/tinyusdz.js",
    "./dist/tinyusdz.wasm",
    "./dist/TinyUSDZLoader.js",
    "./dist/TinyUSDZLoaderUtils.js",
    "./dist/TinyUSDZComposer.js"
  ],
  "scripts": {
    "copy:lic": "cpx '../../LICENSE' './dist/'",
    "copy:em": "cpx '../dist/tinyusdz.*' './dist/'",
    "copy:js": "cpx '../js/src/tinyusdz/**/*' './dist'",
    "copy:all": "npm run copy:lic && npm run copy:em && npm run copy:js",
    "build": "npm run copy:all",
    "build:clean": "rimraf dist && npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "cpx": "^1.5.0",
    "rimraf": "^5.0.0"
  },
